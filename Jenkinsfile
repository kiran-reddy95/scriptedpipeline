pipeline {
    agent any

    stages {
        stage('one') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Two'){
            steps {
                echo 'I am comming to the Scripted Pipeline World'
            }
        }
        stage('There') {
            steps {
                echo 'Finally I got it'
            }
        }
    }
}
